{
  "name": "isa-backend",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/Qualis-Infectologia/ISA-Backend.git",
  "author": "Guilherme Bolfe <guilherme@portalqualis.com.br>",
  "scripts": {
    "production": "rm -rf node_modules dist package-lock && yarn install && yarn build",
    "build": "tsc",
    "postbuild": "cp -R node_modules dist/node_modules && cp package.json dist/package.json && cp infra/build/ormconfig.js dist/shared/infra/typeorm/ormconfig.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpileOnly --ignore-watch node_modules src/shared/infra/http/server.ts",
    "start": "cp .env dist/shared/infra/http/.env && node dist/shared/infra/http/server.js",
    "test": "yarn jest --passWithNoTests",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "install:clean": "rm -rf node_modules/ && rm -rf yarn.lock && yarn install",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync"
  },
  "resolutions": {
    "tslib": "1.11.2"
  },
  "dependencies": {
    "@types/agenda": "2.0.8",
    "@types/express-session": "1.17.0",
    "@types/faker": "4.1.11",
    "@zenvia/sdk": "^1.2.0",
    "@zenvia/zenvia-sms-core": "0.1.0",
    "Faker": "0.7.2",
    "agenda": "3.1.0",
    "agendash": "1.0.0",
    "aws-sdk": "2.689.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "date-fns": "2.14.0",
    "dotenv": "8.2.0",
    "e": "0.0.4",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "express-list-endpoints": "4.0.1",
    "express-session": "1.17.1",
    "handlebars": "4.7.6",
    "jsonwebtoken": "8.5.1",
    "keycloak-admin-client": "0.12.0",
    "keycloak-connect": "10.0.2",
    "keycloak-request-token": "0.1.0",
    "log4js": "6.3.0",
    "log4js-node-mongodb": "2.2.1",
    "module-alias": "2.2.2",
    "moment": "2.24.0",
    "multer": "1.4.2",
    "nodemailer": "6.4.8",
    "nodemailer-express-handlebars": "4.0.0",
    "nodemailer-ses-transport": "1.5.1",
    "nodemon": "2.0.4",
    "pg": "8.0.3",
    "pg-hstore": "2.3.3",
    "reflect-metadata": "0.1.13",
    "request-promise-native": "1.0.8",
    "swagger-ui-express": "4.1.4",
    "ts-node": "8.10.2",
    "tsyringe": "4.2.0",
    "typeorm": "0.2.24",
    "uuid": "7.0.3",
    "uuidv4": "6.0.7",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@babel/preset-env": "7.9.5",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/jsonwebtoken": "8.3.9",
    "@types/multer": "1.4.3",
    "@types/nodemailer": "6.4.0",
    "@types/swagger-ui-express": "4.1.2",
    "@types/yup": "0.28.0",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "babel-jest": "25.4.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "25.4.0",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "25.5.0",
    "ts-node-dev": "1.0.0-pre.44",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3",
    "typeorm-seeding": "1.6.1"
  },
  "_moduleAliases": {
    "@establishments": "modules/establishments/",
    "@protocols": "modules/protocols/",
    "@users": "modules/users/",
    "@notification": "modules/notification/",
    "@security": "modules/security/",
    "@modules": "modules/",
    "@config": "config/",
    "@shared": "shared/",
    "@docs": "docs/",
    "@errors": "shared/errors/",
    "@routes": "shared/infra/http/routes/"
  }
}
